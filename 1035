#include <iostream>
using namespace std;

int main() {
	int A,B,C,D,sum_ab,sum_cd;
	scanf("%d %d %d %d", &A, &B, &C, &D);
	sum_ab = A+B;
	sum_cd = C+D;
	if(B>C && D>A && sum_cd>sum_ab && C>=0 && D>=0 && A%2==0){
		printf("Valores aceitos\n");
			}
	else{
		printf("Valores nao aceitos\n");
	}
	return 0;
}
