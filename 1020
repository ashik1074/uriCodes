#include<stdio.h>
int main()
{
    int d,m=0,y=0;
    scanf("%d",&d);
    if(d>=365){
        y=d/365;
        d=d%365;
        m=d/30;
        d=d%30;
    }
    else if(d<365 || d>=30){
        m=d/30;
        d=d%30;
    }
    printf("%d ano(s)\n",y);
    printf("%d mes(es)\n",m);
    printf("%d dia(s)\n",d);
        return 0;
}
